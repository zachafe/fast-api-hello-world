#PROYECTO DE APIS CON FAST-API

#CREAR ENTORNO VIRTUAL
python3 -m venv venv

#ACTIVAR ENTORNO VIRTUAL
source venv/bin/activate

#INSTALAR FAST-API Y UVICORN ENTORNO VIRTAUL
pip install fastapi uvicorn

#INICIAR APP DESDE CONSOLA SERVER
uvicorn main:app --reload


